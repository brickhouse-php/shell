{
  "name": "brickhouse/shell",
  "description": "Interactive developer shell for Brickhouse applications",
  "license": "MIT",
  "keywords": [
    "framework",
    "brickhouse",
    "mvc"
  ],
  "homepage": "https://github.com/brickhouse-php/brickhouse",
  "support": {
    "issues": "https://github.com/brickhouse-php/brickhouse/issues",
    "source": "https://github.com/brickhouse-php/brickhouse"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Max T. Kristiansen",
      "email": "me@maxtrier.dk",
      "role": "Author",
      "homepage": "https://maxtrier.dk"
    }
  ],
  "require": {
    "php": "^8.4",
    "brickhouse/core": "dev-main",
    "psy/psysh": "^0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Brickhouse\\Shell\\": "src"
    },
    "classmap": [
      "src/"
    ]
  },
  "extra": {
    "brickhouse": {
      "extensions": [
        "\\Brickhouse\\Shell\\Extension"
      ]
    }
  }
}
